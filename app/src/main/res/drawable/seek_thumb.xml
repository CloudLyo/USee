<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape
                android:shape="oval"
                xmlns:android="http://schemas.android.com/apk/res/android">

            <size
                    android:height="24dp"
                    android:width="24dp"/>
            <gradient
                    android:endColor="#b9000000"
                    android:angle="-90"
                    android:startColor="#22FFFFFF"
                    android:type="linear"/>
        </shape>

    </item>
    <item >

        <shape
                android:shape="oval"
                xmlns:android="http://schemas.android.com/apk/res/android">
            <solid android:color="@color/ali_feedback_color_white"/>
            <padding android:right="2dp" android:top="2dp" android:bottom="2dp" android:left="2dp"/>
            <gradient
                    android:endColor="#17ffffff"
                    android:gradientRadius="12dp"
                    android:startColor="#ffffff"
                    android:type="radial"/>
        </shape>
    </item>

</layer-list>
