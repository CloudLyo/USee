<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item  >
        <layer-list >
            <item >
                <shape
                    android:shape="rectangle" >
                    <stroke android:color="#19d9d8d8" android:width="1dp"/>
                    <padding
                        android:bottom="1dp"
                        android:right="0.5dp"
                        android:left="0.5dp"
                        />
                    <corners android:radius="30dp"/>
                </shape>
            </item>
            <item >
                <shape
                    android:shape="rectangle" >
                    <stroke android:color="#66b9b8b8" android:width="1dp"/>
                    <padding
                        android:bottom="1dp"
                        android:right="0.5dp"
                        android:left="0.5dp"
                        />
                    <corners android:radius="30dp"/>
                </shape>
            </item>
            <item >
                <shape
                    android:shape="rectangle" >
                    <stroke android:color="#90acaaaa" android:width="2dp"/>
                    <padding
                        android:bottom="1dp"
                        android:right="0.5dp"
                        android:left="0.5dp"
                        />
                    <corners android:radius="30dp"/>
                </shape>
            </item>
        </layer-list>
    </item>
    <item >
        <shape xmlns:android="http://schemas.android.com/apk/res/android"
            android:shape="rectangle">
            <solid android:color="#aec8de"/>
            <corners android:radius="30dp"/>
        </shape>
    </item>

</layer-list>
